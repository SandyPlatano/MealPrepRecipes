# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Google OAuth (for Calendar integration)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Resend (Email)
RESEND_API_KEY=your_resend_api_key

# Anthropic (Recipe parsing)
ANTHROPIC_API_KEY=your_anthropic_api_key

# Sentry (Error tracking)
SENTRY_DSN=your_sentry_dsn
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Stripe Payment Processing
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key

# Stripe Price IDs (from your Stripe Dashboard)
NEXT_PUBLIC_STRIPE_PRICE_ID_PRO=price_your_pro_monthly_price_id
NEXT_PUBLIC_STRIPE_PRICE_ID_PREMIUM=price_your_premium_monthly_price_id

# Development Feature Flags
# Set to "true" to enable premium features in development without a subscription
DEVELOPMENT_PREMIUM_OVERRIDE=true
# Set to "true" to enable debug logging output
DEBUG_LOGGING=false
